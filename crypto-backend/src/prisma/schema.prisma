datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Cryptocurrency {
  id        Int      @id @default(autoincrement())
  name      String
  symbol    String
  price     Float
  trend     Float
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
